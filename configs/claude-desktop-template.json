{
  "mcpServers": {
    "frappe": {
      "command": "/path/to/frappe-mcp-server/bin/frappe-mcp-server-stdio",
      "args": [
        "--config",
        "/path/to/config.yaml"
      ],
      "env": {
        "FRAPPE_BASE_URL": "https://YOUR_FRAPPE_DOMAIN.com",
        "FRAPPE_API_KEY": "YOUR_API_KEY",
        "FRAPPE_API_SECRET": "YOUR_API_SECRET",
        "LLM_BASE_URL": "http://localhost:11434/v1",
        "LLM_MODEL": "llama3.2:1b"
      }
    }
  }
}